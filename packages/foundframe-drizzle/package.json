{
  "name": "@o19/foundframe-drizzle",
  "version": "0.1.0",
  "description": "Drizzle ORM implementation of foundframe ports",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "tauri:dev": "tsc --watch",
    "build": "tsc",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js"
    },
    "./adaptors": {
      "types": "./dist/adaptors/index.d.ts",
      "import": "./dist/adaptors/index.js"
    }
  },
  "dependencies": {
    "@o19/foundframe-front": "workspace:*"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "better-sqlite3": "^12.6.2",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "typescript": "5.9.2"
  }
}
